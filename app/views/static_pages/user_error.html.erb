	<div class="span9">	
		<table>
			<tbody>
				<tr>
					<td>
						The category you are looking for is not found.
					</td>
				</tr>
			</tbody>
		</table>
	</div>
	<div class="span3">
		<h3>Browse Jobs</h3>
		<ul>
			<% CATEGORY.each do |choice| %>
			<li><%= link_to "#{choice}", category_users_path(choice) %></li>
			<% end %>
		</ul>
	</div>
</div>
