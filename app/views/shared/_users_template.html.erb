<script type="text/html" id="moreUsersList">
	{{#if usersMoreThanZero}}
	{{#each usersData}}
	<li class="user-preview">
		<a href="/users/{{this.id}}">
			<div class="left pull-left">
					<span class="level">Lvl {{getPoints this.job_pref_pnts "<%= @cat_id %>"}}
					</span>
					<span><img src="{{this.image}}"></span>
					<div>
						<span><b>{{this.first_name}} {{this.last_name}}</b></span>
						<span><em>{{this.personal_statement}}</em></span>
					</div>
			</div>	
			<div class="center pull-left">
				{{getGames this.games}}				
			</div>
		</a>
		<div class="votes">
	    <span class="{{this.vote_color}}">+{{this.vote_count}}</span>
	    <%	if current_user %>
				<a href="#" class="icon-heart {{this.vote_color}} vote-btn" 
						data-vote='{"user_id" : "{{this.id}}", "jobpref": "<%="#{@cat_id}"%>"}'></a>
			<% else %>
				<span><i class="icon-heart {{this.vote_color}}"></i></span>
	  	<% end %>
		</div>
	</li>
	{{/each}}
	<div class="border-dash"></div>	
	{{/if}}
</script>